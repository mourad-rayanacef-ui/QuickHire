{
  "name": "aquickhire-backend",
  "version": "1.0.0",
  "description": "AQuickHire - Express.js backend with Prisma",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "npx prisma generate --schema=./prisma/prisma.config",  
    "dev": "nodemon server.js",
    "prisma:generate": "npx prisma generate --schema=./prisma/prisma.config", 
    "prisma:migrate": "npx prisma migrate dev --schema=./prisma/prisma.config", 
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:deploy": "npx prisma migrate deploy --schema=./prisma/prisma.config"  
  },
  "prisma": {  
    "schema": "./prisma/prisma.config"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "express",
    "prisma",
    "postgresql",
    "rest-api",
    "backend"
  ],
  "author": "AQuickHire Team",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.3.0",
    "axios": "^1.13.4",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "http": "^0.0.1-security",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "mailersend": "^2.6.0",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^4.2.1",
    "node-mailjet": "^6.0.11",
    "nodemailer": "^7.0.13",
    "openai": "^6.17.0",
    "pg": "^8.16.3",
    "puppeteer": "^24.36.0",
    "resend": "^6.9.1",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.0.1",
    "prisma": "^7.3.0"
  }
}